{
  "manifest_version": 2,
 
  "name": "Artists List View for Google Play Music",
  "version": "1.0.0",
  "description": "Displays the Artists page in Google Play Music as a more compact list.",
  "icons": { "48": "icons/icon48.png",
           "96": "icons/icon96.png",
           "128": "icons/icon128.png",
          "256": "icons/icon256.png" },

  "content_scripts": [{
    "matches": ["https://play.google.com/music/*"],
    "css": ["compactliststyles.css"]
  }]
}